num = int(input())
li=[]
for i in range(num):
    x=input()
    s1,s2="",""
    if len(x)%2==0:
        s1=x[:len(x)//2]
        s2=x[len(x)//2:]
    else:
        s1=x[:len(x)//2]
        s2=x[len(x)//2+1:]
    L1=list(s1)
    L2=list(s2)
    L1.sort()
    L2.sort()
    s1=str(L1)
    s2=str(L2)
    if s1==s2:
        print("YES")
    else:
        print("NO")
