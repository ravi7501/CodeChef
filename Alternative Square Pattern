num=int(input())
k=-4
for i in range(num):
    if i%2==0:
        k=k+4 
        for j in range(5):
            print(k+1,end=" ")
            k+=1 
        print()
    else:
            k=k+6
            for j in range(5):
                print(k-1,end=" ")
                k-=1
            print()
        
